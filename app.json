      "googleServicesFile": "./google-services.json",
      "package": "app.acksense.App",
      "permissions":["NOTIFICATIONS","CAMERA","ACCESS_COARSE_LOCATION","ACCESS_FINE_LOCATION"],
      "useNextNotificationsApi":true,
      "config" : {
        "googleMaps" : {"apiKey" : "AIzaSyDcp-Lbw5bftCto6bM-S4xMXvq1TQ2J5iU"}
      }
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
